<h1>Room List</h1>



<table id="book" border="3" layout= "fixed" width=" 100%"  >
<br>
  
  <tr>
    <th>roomnumber</th>
    <th>description</th>
    <th> status</th>
    <th> image</th>
    <th>room type</th>

    <th> Action</th>
   
  </tr>

<% @rooms.each do |room| %>  
  <tr >
    <td><%= room.roomnumber%></td>
    <td><%= room.description %></td>
    <td><%= room.status %></td>
    <td><%=image_tag(room.roomtype.roomimg)%></td>
    <td><%= room.roomtype.roomtype %></td>

  

  </td>

  </tr>
<% end %>
</table>


<li><%= link_to "Sign Out", destroy_customer_session_path, data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure?"
                  } %></li>